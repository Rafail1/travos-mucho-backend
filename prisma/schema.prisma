generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model DepthUpdates {
  time     DateTime
  price    Float
  quantity Float
  type     TradeType
  s        Symbol
  snapshot Boolean?  @default(false)

  @@unique([s, time, price, type])
  @@index([s])
  @@index([time])
  @@index([price])
}

enum TradeType {
  ask
  bid
}

